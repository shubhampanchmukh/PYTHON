# 0 1 1 2 3 5 8 13

print("FIBONACCI SERIES!")

def function_fabonacci1(g):
    if g==0:
        return 0
    elif g==1:
        return 1
    else:
        return function_fabonacci1(g-1)+function_fabonacci1(g-2)




print("ENTER THE NUMBER")
g = int(input())
print("fabonacci series is", function_fabonacci1(g))
